{
    "name": "Unvisited tabs",
    "version": "1",
    "manifest_version": 2,
    "description": "Simple tool to indicate unvisited tabs/",
    "background": {
        "scripts": ["background.js"]
    },
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["content_script.js"],
          "run_at": "document_start"
        }
    ],
    "browser_action": {
      "default_title": "Unvisited tabs",  
      "default_popup": "popup.html" 
  },
    "permissions": [
        "tabs",
        "<all_urls>",
        "storage"
      ]
 }
